{
  "version": 2,
  "builds": [
    { "src": "frontend/**", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/generate", "dest": "/api/generate.js" },
    { "src": "/api/upload-logo", "dest": "/api/upload-logo.js" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
